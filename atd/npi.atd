type npi_response = {
  result_count : int;
  results : result list
}

type result = {
  number : int;
  ?basic : basic option
}

type basic = {
  ?first_name : string option;
  ?authorized_official_first_name: string option;
  ?last_name : string option;
  ?authorized_official_last_name: string option;
  name : string
}